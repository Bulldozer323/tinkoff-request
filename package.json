{
  "name": "@tinkoff/request",
  "version": "0.6.0",
  "description": "Request library extendable by plugins",
  "private": true,
  "workspaces": [
    "packages/**/*",
    "website"
  ],
  "scripts": {
    "test": "jest --coverage",
    "bootstrap": "yarn --frozen-lockfile",
    "build": "lerna run tsc",
    "watch": "lerna run watch --parallel",
    "publish": "lerna publish",
    "clean": "git clean -Xdf .",
    "docs:build": "yarn --cwd ./website build",
    "docs:start": "yarn --cwd ./website start",
    "docs:publish": "yarn --cwd ./website publish-gh-pages"
  },
  "keywords": [
    "request"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^10.11.7",
    "jest": "^24.7.1",
    "lerna": "^3.13.1",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.1",
    "typescript": "^3.4.1"
  }
}
